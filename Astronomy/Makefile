SRCS_fd1 = \
Time.f 

OBJS_fd1 = \
Time.o 

SRC_DIR_fd1 = 
OBJS_DIR = obj/Debug/
EXE_DIR = bin/Debug/

EXE = Astronomy.exe
FC = gcc.exe
LD = g++.exe
IDIR = 
CFLAGS = -Wall -g  -J$(OBJS_DIR) $(IDIR)
LFLAGS = 
LIBS = 

VPATH = $(SRC_DIR_fd1):$(OBJS_DIR)
OBJS = $(addprefix $(OBJS_DIR), $(OBJS_fd1))

all : $(EXE)

$(EXE) : $(OBJS_fd1)
	@mkdir -p $(EXE_DIR)
	$(LD) -o $(EXE_DIR)$(EXE) $(OBJS) $(LFLAGS) $(LIBS)

$(OBJS_fd1):
	@mkdir -p $(OBJS_DIR)
	$(FC) $(CFLAGS) -c $(SRC_DIR_fd1)$(@:.o=.f) -o $(OBJS_DIR)$@

clean :
	rm -f $(OBJS_DIR)*.*
	rm -f $(EXE_DIR)$(EXE)

